<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DsaccoAPP.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DsaccoAPP"
    xmlns:pages="clr-namespace:DsaccoAPP.Pages"
    Shell.FlyoutBehavior="Flyout">
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <FlyoutItem Title="Register">
        <ShellContent ContentTemplate="{DataTemplate pages:Register}"/>
    </FlyoutItem>
            <FlyoutItem Title="Login">
        <ShellContent ContentTemplate="{DataTemplate pages:Login}"/>
    </FlyoutItem>
  

</Shell>
